{
  "expo": {
    "name": "FS Map Tool",
    "description": "A small tool to know where you're flying in MS Flight Simulator 2020.",
    "privacy": "public",
    "slug": "fsmaptool",
    "version": "1.0.4",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.riccardolardi.fsmaptool",
      "buildNumber": "1.0.4",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "package": "com.riccardolardi.fsmaptool",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
